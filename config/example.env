# 数据库连接字符串
# DSN="mysql://user:password@localhost:3306/dbname"
# DSN="postgres://user:password@localhost:5432/dbname?sslmode=disable"
# DSN="sqlite:///path/to/database.db"
# DSN="sqlserver://user:password@localhost:1433/dbname"
# DSN="mongodb://user:password@localhost:27017/dbname"

# 多数据库连接（使用逗号分隔）
# DSN="mysql://user:password@localhost:3306/dbname,postgres://user:password@localhost:5432/dbname,sqlite:///path/to/database.db"
# DSN="mysql://user:password@localhost:3306/dbname,mongodb://user:password@localhost:27017/dbname"

